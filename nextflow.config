    fc_biotype_group_features = 'gene_biotype'
    bam_suffix = "_Aligned.sortedByCoord.out.bam"
    design = '/mnt/d/Zymo/GitHub/nxf-rnaseq/design.csv'
    compare = '/mnt/d/Zymo/GitHub/nxf-rnaseq/comparison.csv'
    deseq2_fdr = 0.05
    deseq2_lfc = 0.585   
    gprofiler_fdr = 0.05
