// params of workflow

params {
    outdir = 4
    reads="/home/quangong/testData/fastq/*_R{1,2}.fastq.gz"
    length=20
    quality=20
    genome="/home/quangong/genome/chr1.fa"
    outdir="$baseDir/result"
    index="/home/quangong/genome/chr1_hg38_index"
